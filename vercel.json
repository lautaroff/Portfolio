{
  "headers": [
    {
      "source": "/(.*\\.pdf)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pdf"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/lautaro_ferreyra_cv.pdf", "destination": "/Lautaro_Ferreyra_cv.pdf" },
    { "source": "/:path*", "destination": "/:path*" }
  ]
}
